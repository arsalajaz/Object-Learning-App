<!DOCTYPE html>
<html lang="en">
<head>
<title> Set Display </title>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/set.css">
</head>

<body>
    <div class="canvas">
    
	<% for(let i=setImages.length; i>0; i--) {%> 
		<div class="mySlides">
		<ul>
			<li>
			<% for(let j=i; j<setImages.length; j++) {%> 
				<img src="<%=setImages[j].url%>" alt="<%=setImages[j].name%>" class="responsive">
			<% } %>
			<% for(let k=0; k<i; k++) {%> 
				<img src="<%=setImages[k].url%>" alt="<%=setImages[k].name%>" class="responsive">
			<% } %>  
			</li>
		</ul>
		</div>
    <% } %> 

    </div>

    <button class="display-left" onclick="plusDivs(-1)">&#10094;</button>
    <div class="display-right" >
    <button  onclick="plusDivs(+1)">&#10095;</button>
    </div>

    <script>

      var slideIndex = 1;
      showDivs(slideIndex);

      function plusDivs(n) {
        showDivs(slideIndex += n);
      }

      function showDivs(n) {
        var i;
        var x = document.getElementsByClassName("mySlides");
        if (n > x.length) {slideIndex = 1}
        if (n < 1) {slideIndex = x.length} ;
        for (i = 0; i < x.length; i++) {
          x[i].style.display = "none";
      }
        x[slideIndex-1].style.display = "block";
      }
    </script>
</body>
</html>